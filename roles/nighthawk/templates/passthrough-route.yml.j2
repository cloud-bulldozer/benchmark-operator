apiVersion: route.openshift.io/v1
kind: Route
metadata:
  name: nighthawk-benchmark-route-passthrough-{{ item }}
  namespace: "{{ operator_namespace }}"
spec:
  port:
    targetPort: https
  tls:
    termination: passthrough
  to:
    kind: Service
    name: nighthawk-benchmark-service-passthrough-{{ item }}