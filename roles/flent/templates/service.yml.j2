---
kind: Service
apiVersion: v1
metadata:
  name: flent-service-{{ item }}-{{ trunc_uuid }}
  namespace: '{{ operator_namespace }}'
  labels:
    app: flent-bench-server-{{ item }}-{{ trunc_uuid }}
    type: flent-bench-server-{{ trunc_uuid }}
spec:
  selector:
    app: flent-bench-server-{{ item }}-{{ trunc_uuid }}
  ports:
  - name: flent
    port: 12866
    targetPort: 12866
